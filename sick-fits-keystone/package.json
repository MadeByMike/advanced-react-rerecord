{
  "name": "sick-fits-keystone",
  "description": "An example KeystoneJS project with a User list and Authentication.",
  "private": true,
  "version": "5.0.0",
  "author": "The KeystoneJS Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=8.4.0"
  },
  "scripts": {
    "//": "TODO: There is no way this works on windows",
    "dev": "nodemon --verbose --exec cross-env NODE_ENV=development DISABLE_LOGGING=true node -r esm `which keystone` dev",
    "build": "cross-env NODE_ENV=production keystone build",
    "start": "cross-env NODE_ENV=production keystone start",
    "test": "NODE_ENV=test jest --watch",
    "heroku-postbuild": "next build"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/frontend/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/frontend/.next/",
      "<rootDir>/frontend/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    }
  },
  "dependencies": {
    "@keystonejs/adapter-mongoose": "^5.1.4",
    "@keystonejs/app-admin-ui": "^5.5.4",
    "@keystonejs/app-graphql": "^5.0.2",
    "@keystonejs/app-next": "^5.0.1",
    "@keystonejs/auth-password": "^5.0.1",
    "@keystonejs/fields": "^6.1.0",
    "@keystonejs/fields-datetime-utc": "^5.0.4",
    "@keystonejs/fields-wysiwyg-tinymce": "^5.0.4",
    "@keystonejs/file-adapters": "^5.3.1",
    "@keystonejs/keystone": "^5.4.2",
    "@keystonejs/list-plugins": "^5.0.2",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "graphql": "^14.5.8",
    "graphql-fields": "^2.0.3",
    "nodemailer": "^6.4.2",
    "stripe": "^8.2.0",
    "@apollo/react-hooks": "^3.1.3",
    "@wesbos/dump": "^0.1.1",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-upload-client": "^12.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.10.6",
    "date-fns": "^2.9.0",
    "downshift": "^4.0.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "lodash.debounce": "^4.0.8",
    "next-with-apollo": "^4.3.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-adopt": "^0.6.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "react-stripe-checkout": "^2.6.3",
    "react-transition-group": "^4.3.0",
    "styled-components": "4.4.1",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "casual": "^1.6.2",
    "enzyme-to-json": "^3.4.3",
    "graphql-tools": "^4.0.6",
    "jest": "^24.9.0",
    "jest-transform-graphql": "^2.1.0"
  },
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
